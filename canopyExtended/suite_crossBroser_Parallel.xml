    <classes>
      <class name="com.comcast.crm.orgTest.CreateOrganisationTest"/>
      <class name="com.comcast.crm.orgTest.DeleteOrg_Test"/>
      <class name="com.comcast.crm.contactTest.CreateContactTest"/>
